#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/rc@1.2.8/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/node_modules:/Users/aaratrikachakraborty/Documents/node_modules:/Users/aaratrikachakraborty/node_modules:/Users/node_modules:/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules/rc/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules/registry-auth-token/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/rc@1.2.8/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/node_modules:/Users/aaratrikachakraborty/Documents/node_modules:/Users/aaratrikachakraborty/node_modules:/Users/node_modules:/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules/rc/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules:/Users/aaratrikachakraborty/Documents/Ecomm/heinz-95729-project-main/web/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules/registry-auth-token/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../rc/cli.js" "$@"
else
  exec node  "$basedir/../../../rc/cli.js" "$@"
fi
